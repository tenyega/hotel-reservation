{% extends "front/base.html.twig" %}
{% block title %}
	Confirmation
{% endblock %}
{% block javascripts %}
	<link href='../node_modules/fullcalendar-scheduler/main.css' rel='stylesheet'/>
	<script src='../node_modules/fullcalendar-scheduler/main.js'></script>
	<script>
		function call() {
		console.log('hi inside the code');
		var calendarEl = document.getElementById('calendar');
		var calendar = new FullCalendar.Calendar(calendarEl, {
		timeZone: 'UTC',
		initialView: 'resourceTimelineWeek',
		aspectRatio: 1.5,
		headerToolbar: {
		left: 'prev,next',
		center: 'title',
		right: 'resourceTimelineDay,resourceTimelineWeek,resourceTimelineMonth'
		},
		editable: true,
		resourceAreaHeaderContent: 'Rooms',
		resources: [
		{
		id: 'a',
		title: 'Room 1'
		}, {
		id: 'b',
		title: 'Room 2'
		}
		],
		events: 'https://fullcalendar.io/api/demo-feeds/events.json?single-day&for-resource-timeline'
		});

		calendar.render();
}
	</script>
{% endblock %}
{% block body %}
	{# <button class="btn-primary" onclick="call()">
			click</button> #}
	<div id='calendar'></div>
	<h1>Votre reservation est confirmé</h1>
	<h2>Voici les récapitulative de votre reservation</h2>

{% endblock %}
