<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" href="Img/logo.png"/>


		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}

			<link rel="stylesheet" href="{{ asset('CSS/Theme.css') }}"/>
			<link rel="stylesheet" href="{{ asset('CSS/_navbar.css') }}"/>

			{% include "./partial/_navbar.html.twig" %}

		{% endblock %}

		{% block javascripts %}
			<script src="../JS/ImageSlider.js" type="text/javascript"></script>
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
	</body>
</html>
